RaidChip.FocusLost:Connect(function(enterPressed)
    local chip = RaidChip.Text
    print("Chip value:", chip)  -- Debugging line

    local args = {
        [1] = "RaidsNpc",
        [2] = "Select",
        [3] = chip
    }

    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))

task.wait(0.1)

    local CoreGui = game:GetService("StarterGui")
    CoreGui:SetCore("SendNotification", {
        Title = "Raid Chip Notification",
        Text = "You Have Buy It, Raid Chip: " .. chip .. ", If no that means you does not have enough level/money !",
        Duration = 1,
    })

task.wait(1)

    fireclickdetector(workspace.Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)

local CoreGui = game:GetService("StarterGui")
    CoreGui:SetCore("SendNotification", {
        Title = "Raid Chip Used",
        Text = "Loading Map: " .. chip .. ", Please Wait !",
        Duration = 1,
    })
end)

