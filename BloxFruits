-- Quick Tools --

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function onPlayerChatted(message)
    local words = string.split(message, " ")

    if words[1] == "Buy" and words[2] then
        local itemName = words[2]
        wait(0.2)
        local args = {
            [1] = "BuyItem",
            [2] = itemName
        }
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))

    elseif words[1] == "Get" and words[2] then
        local toolName = words[2]
        wait(0.2)
        local args = {
            [1] = "LoadItem",
            [2] = toolName
        }
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))

    elseif words[1] == "BuyF" and words[2] then
        local fruitName = words[2]
        wait(0.1)
        local args = {
            [1] = "PurchaseRawFruit",
            [2] = fruitName,
            [3] = false
        }
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))

    elseif words[1] == "Shop" then
        game:GetService("Players").LocalPlayer.PlayerGui.Main.FruitShop.Visible = true
    elseif words[1] == "Awk" then
        game:GetService("Players").LocalPlayer.PlayerGui.Main.AwakeningToggler.Visible = true
    elseif words[1] == "Titles" then
        game:GetService("Players").LocalPlayer.PlayerGui.Main.Titles.Visible = true
    end
end

player.Chatted:Connect(onPlayerChatted)
