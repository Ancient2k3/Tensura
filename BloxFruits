    local inputValue = tonumber(ChangeDashLength.Text)
    if inputValue then
        local character = game.Workspace.Characters[myself.Name]
        character:SetAttribute("DashLength", inputValue)
    else
        warn("Missing DashLength Value Number.")
    end
end)

