local function RaceActivated()
local args = {
    [1] = "RaceSkill"
}

game:GetService("ReplicatedStorage").OtherEvent.SkillEvents.Movement:FireServer(unpack(args))
wait(0.01)
end

local function moveItem(itemName)
    local droppedItems = game:GetService("Workspace"):FindFirstChild("Dropped_Items")
    if droppedItems then
        local item = droppedItems:FindFirstChild(itemName)
        if item and item:FindFirstChild("Handle") then
            item.Handle.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        else
            warn(itemName .. " not found in the game!")
        end
    else
        warn("Dropped_Items not found in Workspace!")
    end
end

local function collectItems()
    local itemNames = {
        "Invisible Power", "Fly Power", "Bomb Power", "Spin Power", "Barrier Power",
        "Diamond Power", "Paw Power", "Water Power", "Gold Power", "Sand Power",
        "Flame Power", "Snow Power", "Ice Power", "Moai Power",
        "Dough Power", "Dog Power", "Dark Power", "Floppa Power"
    }

    for _, itemName in ipairs(itemNames) do
        moveItem(itemName)
    end
end

