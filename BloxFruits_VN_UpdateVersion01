-- Removed Pre-existing GUI --
local Previous_SG_Name = "NganHa2k3_BFV01"

for _, SG in ipairs(game:GetService("CoreGui"):GetChildren()) do
    if SG:IsA("ScreenGui") and SG.Name == Previous_SG_Name then
        pcall(function()
            SG:Destroy()
        end)
    end
end

