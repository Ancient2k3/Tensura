function FastAttack_Status()
    if not FA_Toggle and not FA_Toggle_II then
        FA_Toggle = true
        if _FAmode == 1 then
            if _G.EnableFastAttack_Players == true then
                _G.EnableFastAttack_Players = false
            end
            _G.EnableFastAttack_Enemies = true
        elseif _FAmode == 2 then
            if _G.EnableFastAttack_Enemies == true then
                _G.EnableFastAttack_Enemies = false
            end
            _G.EnableFastAttack_Players = true
        end
        _Notice("Commands", "Fast Attacking Normal is On/Activated!", 1, "SendNotification")
    else
        FA_Toggle = false
        if _FAmode == 1 then
            if _G.EnableFastAttack_Players == true then
                _G.EnableFastAttack_Players = false
            end
            _G.EnableFastAttack_Enemies = false
        elseif _FAmode == 2 then
            if _G.EnableFastAttack_Enemies == true then
                _G.EnableFastAttack_Enemies = false
            end
            _G.EnableFastAttack_Players = false
        end
        _Notice("Commands", "Fast Attacking Normal is Off/Deactivated!", 1, "SendNotification")
    end
end

function SuperFastAttack_Status()
    if not FA_Toggle_II and not FA_Toggle then
        FA_Toggle_II = true
        if _FAmode == 1 then
            if _G.EnableFastAttack_Players == true then
                _G.EnableFastAttack_Players = false
            end
            if _G.SuperFastAttack_Players == true then
                _G.SuperFastAttack_Players = false
            end
            _G.EnableFastAttack_Enemies = true
            _G.SuperFastAttack_Enemies = true
        elseif _FAmode == 2 then
            if _G.EnableFastAttack_Enemies == true then
                _G.EnableFastAttack_Enemies = false
            end
            if _G.SuperFastAttack_Enemies == true then
                _G.SuperFastAttack_Enemies = false
            end
            _G.EnableFastAttack_Players = true
            _G.SuperFastAttack_Players = true
        end
        _Notice("Commands", "Fast Attacking Overpower is On/Activated!", 1, "SendNotification")
    else
        FA_Toggle_II = false
        if _FAmode == 1 then
            if _G.EnableFastAttack_Players == true then
                _G.EnableFastAttack_Players = false
            end
            if _G.SuperFastAttack_Players == true then
                _G.SuperFastAttack_Players = false
            end
            _G.EnableFastAttack_Enemies = false
            _G.SuperFastAttack_Enemies = false
        elseif _FAmode == 2 then
            if _G.EnableFastAttack_Enemies == true then
                _G.EnableFastAttack_Enemies = false
            end
            if _G.SuperFastAttack_Enemies == true then
                _G.SuperFastAttack_Enemies = false
            end
            _G.EnableFastAttack_Players = false
            _G.SuperFastAttack_Players = false
        end
        _Notice("Commands", "Fast Attacking Overpower is Off/Deactivated!", 1, "SendNotification")
    end
end

function CorrectedEveryCommands(boolean_ix)
    local GetCommands_Words = _GetCommands.Text:lower()
    local Matching_Words, Matching_Words_I = nil, nil

    if GetCommands_Words ~= "" then
        for command, aliases in pairs(Commands_DataStorage) do
            for _, alias in ipairs(aliases) do
                if alias:lower():find(GetCommands_Words) then
                    Matching_Words = command
                    Matching_Words_I = command .. ", " .. table.concat({table.unpack(aliases, 2)}, ", ")
                    break
                end
            end
            if Matching_Words then break end
        end
    else
        Matching_Words = ""
        Matching_Words_I = "</>"
    end

    if boolean_ix then
        if Matching_Words_I then
            _GetCorrectedCommands.Text = Matching_Words_I
        else
            _GetCorrectedCommands.Text = _G.Commands_Script_Language_Test[4] or "Seems like it's not a valid command"
        end
    else
        if Matching_Words then
            _GetCommands.Text = Matching_Words
        else
            _GetCommands.Text = ""
        end
    end
end

function _AddCornerUI(Name, Radius)
    local UIC_Zero = Instance.new("UICorner", Name)
    UIC_Zero.CornerRadius = UDim.new(Radius, 0)
end

function _ThemeChanged(RGB_Color)
    _GetCommands.BackgroundColor3 = RGB_Color
    _GetCorrectedCommands.BackgroundColor3 = RGB_Color
    _GetFocusedButton.BackgroundColor3 = RGB_Color
    _SettingsFrame.BackgroundColor3 = RGB_Color
    _SettingsScroll.BackgroundColor3 = RGB_Color
end

function _AddedSettings_NormalB(NameSetting, Scripts)
    local _SpecialNew_Button = Instance.new("TextButton", _SettingsScroll)
    _SpecialNew_Button.BackgroundTransparency = 0
    _SpecialNew_Button.BackgroundColor3 = intBGC_All
    _SpecialNew_Button.Position = UDim2.new(0.02, 0, 0, _SSCRL_Canvasmain + _ButtonPadding)
    _SpecialNew_Button.Size = UDim2.new(0.96, 0, 0, _ButtonHeight)
    _SpecialNew_Button.TextScaled = true
    _SpecialNew_Button.TextSize = 9
    _SpecialNew_Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    _SpecialNew_Button.Font = Enum.Font.Code
    _SpecialNew_Button.Text = NameSetting
    _SpecialNew_Button.Visible = true
    _AddCornerUI(_SpecialNew_Button, 0.2)
    _MouseButton1Click(_SpecialNew_Button, function()
        Scripts()
    end)
    _SSCRL_Canvasmain = _SSCRL_Canvasmain + _ButtonHeight + _ButtonPadding
    _SettingsScroll.CanvasSize = UDim2.new(0, 0, 0, _SSCRL_Canvasmain)
end

_MouseButton1Click(_GetFocusedButton, function()
    if _GetFocusedButton.Text == "<" and _GetCommands.Position == EndPoint then
        _GetFocusedButton.Text = ">"
        print("You're used button to closed activated cmdbar =))")
        OnFocused_SlideIn:Play()
    else
        _GetCommands:CaptureFocus()
    end
end)

_FocusLost(_GetCommands, enterPressed, function()
    print("You're leave activated cmdbar... and have done something!")
    Cmdbar_ItsOnFocused = false
    OnFocused_SlideIn:Play()
    _GetFocusedButton.Text = _G.Commands_Script_Language_Test[2] or ">"
    -- CorrectedEveryCommands(false)
    pcall(EveryCommands)
end)

_GetCommands.Focused:Connect(function()
    print("You're on activated cmdbar, now waiting...")
    Cmdbar_ItsOnFocused = true
    OnFocused_SlideOut:Play()
    _GetFocusedButton.Text = _G.Commands_Script_Language_Test[3] or "<"
end)

_RS.RenderStepped:Connect(function(deltaTime)
    if Cmdbar_ItsOnFocused then
        CorrectedEveryCommands(true)
    end
end)

_RS.RenderStepped:Connect(function()
    if _AutoStatus ~= false then
        local current_points = tonumber(_PLR.Data.Points.Value) or 0
        if current_points ~= 0 then
            _RlS.Remotes.CommF_:InvokeServer("AddPoint", _SpecialVariables.UpStatTarget, current_points)
        end
    end
end)

_RS.RenderStepped:Connect(function()
    if _AutoEnablePvp then
        _EnablePvpOne += 1
        if _EnablePvpOne >= 20 and _PLR:GetAttribute("PvpDisabled") == true then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EnablePvp")
            _EnablePvpOne = 0
        end
    end
end)

_RS.RenderStepped:Connect(function()
    if _SpecialVariables.CheckingUpdate ~= false then
        _SpecialVariables.Reset_Seconds += 1
        if _SpecialVariables.Reset_Seconds >= 2400 then _SpecialVariables.Reset_Seconds = 0
            local _NewestVersion = loadstring(game:HttpGet("https://raw.githubusercontent.com/Ancient2k3/Status/refs/heads/main/Games/BFCommands_UPD.luau"))()
            print(" [This is Important!] \n [Checking Update]: " .. _NewestVersion)
            if _CurrentVersion < _NewestVersion then
                _TPS:Teleport(game.PlaceId, _PLR)
                task.spawn(function() _PLR:Kick("ĐANG UPDATE BẢN CẬP NHẬP SCRIPT MỚI!") end)
            end
        end
    end
end)

_RS.RenderStepped:Connect(function(deltaTime)
    if _SpecialVariables.Farm_NormalNPC then
        _ToNearbyMobs()
    end
end)

_RS.RenderStepped:Connect(function(deltaTime)
    if _SpecialVariables.Farm_BossesNPC then
        _ToNearbyBoss()
    end
end)

_RS.RenderStepped:Connect(function(deltaTime)
    if not _SpecialVariables.Farm_NormalNPC and not _SpecialVariables.Farm_BossesNPC and _PLR.Character and _PLR.Character:FindFirstChild("Humanoid") then
        local _updatedhealth = _PLR.Character.Humanoid.Health
        if _updatedhealth <= _HalfHealth then
            if not _SpecialVariables.Is_LowHealth then
                RaphaelORB.CFrame = CFrame.new(_PLR.Character.HumanoidRootPart.Position)
                _spectatepath(RaphaelORB)
                _SpecialVariables.Is_LowHealth = true
                print("[SAFE MODE]: You're Getting Low Health!\nCurrent HP: " .. _updatedhealth)
                _Notice("Safety Mode", "Low Health Detected, Goto SAFE ZONE!\nCurrent HP: " .. _updatedhealth, 1, "SendNotification")
            end
        else
            if _SpecialVariables.Is_LowHealth then
                RaphaelORB.CFrame = CFrame.new(0, 99999, 0)
                _spectatepath(_PLR.Character.Humanoid)
                _SpecialVariables.Is_LowHealth = false
                print("[SAFE MODE]: That Better!\nCurrent HP: " .. _updatedhealth)
                _Notice("Safety Mode", "Health Restored, Back to Surface!\nCurrent HP: " .. _updatedhealth, 1, "SendNotification")
            end
        end
    end _updatepart(_PLR, Safe_Pad)
end)

_RS.RenderStepped:Connect(function()
    if _SpecialVariables.Farm_NormalNPC or _SpecialVariables.Farm_BossesNPC then
        if _PLR.Character and _PLR.Character:FindFirstChild("HumanoidRootPart") then
            local _WingX = _PLR.Character.HumanoidRootPart:FindFirstChild("Extra_Wings_IX")
            if not _WingX then
                _BV("true")
                _Noclip(true)
            end
        end
    end
end)

_RS.RenderStepped:Connect(function()
    local _HumanoidRP = _PLR.Character:FindFirstChild("HumanoidRootPart")
    local _OniRealmPart = _WS:FindFirstChild(_HHxScripts):FindFirstChild("OniRealmEvents_Island")
    if _HumanoidRP and _OniRealmPart then
        if (_OniRealmPart.Position - _HumanoidRP.Position).magnitude <= _SpecialVariables.OniRealm_CheckDistance then
            if not _SpecialVariables.InOniRealm_Islands then
                _SpecialVariables.InOniRealm_Islands = true
                print("---- You're Joined Oni Realm Dimension! ----")
                _Notice("Oni Realm", "You Are Inside Oni Realm Event Islands!", 1, "SendNotification")
            end
        else
            if _SpecialVariables.InOniRealm_Islands then
                _SpecialVariables.InOniRealm_Islands = false
                print("---- You're Leaving Oni Realm Dimension! ----")
                _Notice("Oni Realm", "You Are Outside Oni Realm Event Islands!", 1, "SendNotification")
            end
        end
    end
end)

_AddCornerUI(_GetCommands, 0.2)
_AddCornerUI(_GetFocusedButton, 0.2)
_AddCornerUI(_GetCorrectedCommands, 0.2)
_AddCornerUI(_SettingsFrame, 0.05)
_AddCornerUI(_SettingsScroll, 0.2)

