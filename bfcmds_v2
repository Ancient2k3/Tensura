function _MouseButton1Click(Name, Scripts)
    Name.MouseButton1Click:Connect(function()
        Scripts()
    end)
end

function _FocusLost(Name, Var, Scripts)
    Name.FocusLost:Connect(function(Var)
        if Var then
            Scripts()
        end
    end)
end

function _BV(State)
    if not _PLR.Character.HumanoidRootPart:FindFirstChild("Extra_Wings_IX") then
        if State == "true" then
            local Safety_First = Instance.new("BodyVelocity", _PLR.Character.HumanoidRootPart)
            Safety_First.Name = "Extra_Wings_IX"
            Safety_First.Velocity = Vector3.new(0, 0, 0)
            Safety_First.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            --warn(":3 Safety flight.")
        end
    else
        if State == "false" then
            if _PLR.Character.HumanoidRootPart:FindFirstChild("Extra_Wings_IX") then
                _PLR.Character.HumanoidRootPart:FindFirstChild("Extra_Wings_IX"):Destroy()
            end
            --warn("=-= becareful.")
        end
    end
end

function _AutoCorrectName(input)
    for _, player in ipairs(_PLRS:GetPlayers()) do
        if string.lower(player.Name):sub(1, #input) == string.lower(input) or
        string.lower(player.DisplayName):sub(1, #input) == string.lower(input) then
            return player
        end
    end
    return nil
end

function _Noclip(_boolean)
    for _, _N in pairs(_PLR.Character:GetDescendants()) do
        if _N:IsA("BasePart") then
            if _boolean == true then
                _N.CanCollide = false
                --warn(":0 noclipping on.")
            elseif _boolean == false then
                _N.CanCollide = true
                --warn(":l noclipping off.")
            end
        end
    end
end

function _Part(_cframe, _name)
    if not _WS:FindFirstChild(_HHxScripts) then
        local instancefolder_x = Instance.new("Folder", _WS)
        instancefolder_x.Name = _HHxScripts
        print("Creating: Folder")
    else
        print("Loaded instance to: Storage")
    end
    
    local epoint = Instance.new("Part")
    epoint.Parent = _WS:FindFirstChild(_HHxScripts)
    epoint.CFrame = _cframe
    epoint.Size = Vector3.new(0.1, 0.1, 0.1)
    epoint.CanCollide = false
    epoint.Anchored = true
    epoint.Color = Color3.new(0, 1, 1)
    epoint.Transparency = 0.9
    epoint.Name = _name
end

function _Dur(_Start, _End, _SP)
    local _DE = (_End - _Start).magnitude
    return _DE / _SP
end

function _Info(_Start, _End)
    local _DR = _Dur(_Start, _End, _TweeningSpeed)
    return TweenInfo.new(_DR, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
end

function _ToPart(_name, _h)
    local _SPart = _WS:FindFirstChild(_HHxScripts):FindFirstChild(_name)
    if _SPart then
        _BV("false")
        _CT_Cancel()
        _BV("true")
        _Noclip(true)
        local _Goal = {CFrame = _SPart.CFrame + _h}
        local _I4 = _Info(_PLR.Character.HumanoidRootPart.Position, _Goal.CFrame.Position)
        _CurrentTween = _TS:Create(_PLR.Character.HumanoidRootPart, _I4, _Goal)
        _CurrentTween:Play()
        _CurrentTween.Completed:Wait()
        _BV("false")
        _Noclip(false)
        -- local _RunningCheck = _RS.RenderStepped:Connect(function()
            if (_PLR.Character.HumanoidRootPart.Position - _SPart.CFrame.Position).Magnitude <= 20 then
                _BV("false")
                _Noclip(false)
                -- _RunningCheck:Disconnect()
                warn("Check if it work!")
            else
                warn("Not in range, it not work :(")
            end
        -- end)
    else
        _Notice("Commands", "Error not found Islands named: " .. _name, 1, "SendNotification")
    end
end

function _Spectating(players_camera)
    if players_camera and players_camera.Character and players_camera.Character:FindFirstChild("HumanoidRootPart") then
        local dcamera = game:GetService("Workspace").CurrentCamera
        dcamera.CameraSubject = players_camera.Character.HumanoidRootPart
        dcamera.CFrame = players_camera.Character.HumanoidRootPart.CFrame
        print("Successfully spectating: " .. players_camera.DisplayName)
    else
        print("Not found Humanoid of: " .. players_camera.DisplayName)
    end
end

function _TwPos(_p)
    local _PLR_Position = _PLR.Character.HumanoidRootPart.Position
    local _Goal = {CFrame = CFrame.new(_p)}
    local _I4 = _Info(_PLR_Position, _Goal.CFrame.Position)
    _CurrentTween = _TS:Create(_PLR.Character.HumanoidRootPart, _I4, _Goal)
    _CurrentTween:Play()
end

function _ToPos(_Pos, _Number)
    local _TryTimes = _Number or 3
    local _Totals_Tries = 0
    for i = 1, _TryTimes do
        _Totals_Tries += 1
        print("Trying: " .. _Totals_Tries)
        _PLR.Character.HumanoidRootPart.CFrame = _Pos
    end
end

function _ToNearbyMobs()
    local _NearestEnemies, _Huge, _InRange, _Range, _CurrentPLR_Health, _SafeSpot
    _NearestEnemies = nil
    _Huge = math.huge
    _InRange = 1000
    
    for _, _Enemies in pairs(_WS.Enemies:GetChildren()) do
        if _SpecialVariables.Farm_LightningEvent ~= true then
            if _Enemies:IsA("Model") and _Enemies:FindFirstChild("HumanoidRootPart") then
                _Range = (_PLR.Character.HumanoidRootPart.Position - _Enemies.HumanoidRootPart.Position).Magnitude
                
                if _Range < _Huge and _Range <= _InRange then
                    _Huge = _Range
                    _NearestEnemies = _Enemies
                end
            end
        else
            if _Enemies:IsA("Model") and _Enemies.Name:lower():match("electrified") and _Enemies:FindFirstChild("HumanoidRootPart") then
                _Range = (_PLR.Character.HumanoidRootPart.Position - _Enemies.HumanoidRootPart.Position).Magnitude
                
                if _Range < _Huge and _Range <= _InRange then
                    _Huge = _Range
                    _NearestEnemies = _Enemies
                end
            end
        end
    end
    
    if _PLR.Character and _PLR.Character:FindFirstChild("Humanoid") then
        _CurrentPLR_Health = _PLR.Character.Humanoid.Health
        if _CurrentPLR_Health <= _HalfHealth then
            _SafeSpot = Vector3.new(0, 900, 0)
        else
            _SafeSpot = Vector3.new(0, 25, 0)
        end
    end
    
    if _NearestEnemies and _NearestEnemies:FindFirstChild("Humanoid") then
        local _H, _EnemiesPosition, _DirectionBehind, _PositionBehind
        _H = _NearestEnemies.Humanoid
        _EnemiesPosition = _NearestEnemies.HumanoidRootPart.Position
        _DirectionBehind = (_PLR.Character.HumanoidRootPart.Position - _EnemiesPosition).unit
        _PositionBehind = _EnemiesPosition + (_DirectionBehind * -0.2) + _SafeSpot
        
        if _H.Health > 0 then
            _TwPos(_PositionBehind)
        else
            _NearestEnemies.HumanoidRootPart.CFrame = _PLR.Character.HumanoidRootPart.CFrame * CFrame.new(0, 900, 0)
        end
    end
end

function _ToNearbyBoss()
    local near, ishuge, inrange, instorage, range, checkinghealth, safeheight
    near = nil
    ishuge = math.huge
    inrange = 100000
    instorage = _Worlds:FindFirstChild("EnemySpawns")
    for _, enemy in pairs(instorage:GetChildren()) do
        if enemy:IsA("Part") and enemy.Name:lower():match("boss") then
            range = (_PLR.Character.HumanoidRootPart.Position - enemy.Position).Magnitude
            if range < ishuge and range <= inrange then
                ishuge = range
                near = enemy
            end
        end
    end
    
    if _PLR.Character and _PLR.Character:FindFirstChild("Humanoid") then
        checkinghealth = _PLR.Character.Humanoid.Health
        if checkinghealth <= _HalfHealth then
            safeheight = Vector3.new(0, 900, 0)
        else
            safeheight = Vector3.new(0, 25, 0)
        end
    end
    
    if near then
        local direction, behindpos
        direction = (_PLR.Character.HumanoidRootPart.Position - near.Position).unit
        behindpos = near.Position + (direction * -0.2) + safeheight
        _TwPos(behindpos)
    end
end

function _CT_Cancel()
    if _CurrentTween then
        _CurrentTween:Cancel()
    end
end

function _ReturnOwnerBoatIs(folder)
    local model = nil
    for a, b in pairs(folder:GetChildren()) do
        if b:IsA("Model") then
            local _objv = b:FindFirstChild("Owner")
            if _objv and _objv.Value.Name == _PLR.Name then
                model = _objv.Parent
            end
        end
    end
    return model
end

function _ReturnPlayerPos(name)
    local usertargeted = _PLRS[name].Character:FindFirstChild("HumanoidRootPart")
    if usertargeted then
        return usertargeted.Position
    end
    return nil
end

function _TpBoat_ToPos(pos)
    local _owner_boat = _ReturnOwnerBoatIs(_StoredBoats)
    local _IsBoatName = _owner_boat.Name or "N/A"
    print("Boat founded: " .. _IsBoatName)
    print("Owner Name: " .. _owner_boat:FindFirstChild("Owner").Value.Name)
    if _IsBoatName ~= "N/A" then
        _owner_boat.PrimaryPart.Position = pos
    else
        print("No boat to teleported!")
    end
end

function _Notice(Tt, tx, dur, x)
    if x == "SendNotification" then
        _StGui:SetCore(x, {
            Title = Tt,
            Text = tx,
            Duration = dur,
        })
    elseif x == "DevConsoleVisible" then
        _StGui:SetCore(x, true)
    end
end

function _Clipboard(_m)
    local _someTypeOfClipboard = setclipboard or toclipboard or set_clipboard or (Clipboard and Clipboard.set)
    if _someTypeOfClipboard then
        _someTypeOfClipboard(tostring(_m))
        _Notice("Clipboard", "Saved to clipboard!\nText: " .. _m, 1, "SendNotification")
    else
        _Notice("Clipboard", "Your exploit does not supported, saving clipboard!", 1, "SendNotification")
    end
end

function _InvokeServer(Name, Worlds, ...)
    local _ArGs_Text = {...}
    _RlS.Remotes[Name]:InvokeServer(unpack(_ArGs_Text))
    task.spawn(function()
        _Notice("World Commands", "Teleportation to " .. Worlds .. ". !", 1, "SendNotification")
    end)
end

function _GetFightingStyle(Name, FSName, ...)
    local _ArGs_Text = {...}
    _RlS.Remotes[Name]:InvokeServer(unpack(_ArGs_Text))
    task.spawn(_Notice, "Fighting Style [Switched to]", "Current FS: " .. FSName, 1, "SendNotification")
end

function _PasteScripts(url, scripts)
    local success, result = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    
    if success then
        scripts()
    else
        print(result .. " <failed>!")
    end
end

