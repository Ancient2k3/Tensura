}

function EveryCommands()
    local GetWords = _GetCommands.Text
    local DE_WorldVoice = {}
    
    for wis in string.gmatch(GetWords, "%S+") do
        table.insert(DE_WorldVoice, wis)
    end
    
    if table.find(Commands_DataStorage["reset"], DE_WorldVoice[1]) then
        -- _He:ChangeState(Enum.HumanoidStateType.Dead)
        if _PLR.Character and _PLR.Character:FindFirstChild("Humanoid") then
            _PLR.Character.Humanoid.Health = 0
        end
        _Notice("Commands", "Character Reseted!", 1, "SendNotification")
    elseif table.find(Commands_DataStorage["test"], DE_WorldVoice[1]) then
        loadstring(game:HttpGet(TestTable[1]))()
    elseif table.find(Commands_DataStorage["togglefly"], DE_WorldVoice[1]) then
        if not _FlyIsLoaded then _FlyIsLoaded = true
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Ancient2k3/Tensura/refs/heads/main/FLY.luau"))()
        end
        if not _SpecialVariables.Is_Flying then _SpecialVariables.Is_Flying = true
            _Notice("Commands", "Fly Activated, Speed: " .. DE_WorldVoice[2], 1, "SendNotification")
        else _SpecialVariables.Is_Flying = false
            _Notice("Commands", "Fly Deactivated!", 1, "SendNotification")
        end _G.Script_FlyingEnabled = _SpecialVariables.Is_Flying or false
        _G.Script_FlyingSpeed = DE_WorldVoice[2] or 1
    elseif table.find(Commands_DataStorage["exp"], DE_WorldVoice[1]) then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/HoangHienXScripts/Scripts/refs/heads/chocolate/BloxFruits/AutoRedeemCodes_BF.luau"))()
        _Notice("Commands", "Used Codes x2 Expierence!", 1, "SendNotification")
    elseif table.find(Commands_DataStorage["sellfish"], DE_WorldVoice[1]) then
        _RlS.Modules.Net:FindFirstChild("RF/JobsRemoteFunction"):InvokeServer("FishingNPC", "SellCorruptedFish")
        _Notice("Commands", "ALL Corrupted Fish has been Converted to Oni Token!", 1, "SendNotification")
    elseif table.find(Commands_DataStorage["buyfruit"], DE_WorldVoice[1]) then
        local current_mn, money_lost = _PLR.Data.Beli.Value or 0
        if DE_WorldVoice[2] then
            _RlS.Remotes.CommF_:InvokeServer("PurchaseRawFruit", DE_WorldVoice[2], false)
            money_lost = current_mn - _PLR.Data.Beli.Value or 0
            _Notice("Commands", "Purchased " .. DE_WorldVoice[2] .. " for " .. money_lost, 1, "SendNotification")
        else
            _Notice("Commands", "What fruit do u wanna buy? writing name...", 1, "SendNotification")
        end
    elseif table.find(Commands_DataStorage["togglesafemode"], DE_WorldVoice[1]) then
        _SpecialVariables.Safemode_Enabled = _ReverseBOOLEAN(_SpecialVariables.Safemode_Enabled)
        if _SpecialVariables.Safemode_Enabled then
            _Notice("Commands", "Health safety mode is enabled!", 1, "SendNotification")
        else
            _Notice("Commands", "Health safety mode is disabled!", 1, "SendNotification")
        end
    elseif table.find(Commands_DataStorage["fakestat"], DE_WorldVoice[1]) then
        if DE_WorldVoice[2] and DE_WorldVoice[3] then
            _FakeStatus(DE_WorldVoice[2], DE_WorldVoice[3])
        else
            _Notice("Commands", "Missing Type or Value to do fake status!", 1, "SendNotification")
        end
    elseif table.find(Commands_DataStorage["onirealm"], DE_WorldVoice[1]) then
        _ToPart("MiddleTown_Island", Vector3.new(0, 9, 0))
        task.wait(0.05)
        _RlS.Modules.Net:FindFirstChild("RF/OniTempleTransportation"):InvokeServer("InitiateTeleportToTemple")
        _Notice("Commands", "Teleported to Oni Realm Dimension!", 1, "SendNotification")
    elseif table.find(Commands_DataStorage["randomfruit"], DE_WorldVoice[1]) then
        _RlS.Remotes.CommF_:InvokeServer("Cousin", "BuyRedHead")
        _RlS.Remotes.CommF_:InvokeServer("Cousin", "BuySummer")
        _RlS.Remotes.CommF_:InvokeServer("Cousin", "Buy")
        _Notice("Commands", "Randomized Fruits\n >> Successfully Tried <<", 1, "SendNotification")
    elseif table.find(Commands_DataStorage["tweeningspeed"], DE_WorldVoice[1]) then
        if DE_WorldVoice[2] ~= "" or DE_WorldVoice[2] ~= nil then
            --_Notice("Commands", "Tweening Speed Unchanged!", 1, "SendNotification")
        --else
            local new_TweeningSpeedValue = tonumber(DE_WorldVoice[2])
            _TweeningSpeed = new_TweeningSpeedValue
            _Notice("Commands", "Tweening Speed Changed to " .. _TweeningSpeed, 1, "SendNotification")
        end
    elseif table.find(Commands_DataStorage["spectate"], DE_WorldVoice[1]) then
        if not _IsSpectatingSomeOne then
            _IsSpectatingSomeOne = true
            if DE_WorldVoice[2] then _Spectating(_AutoCorrectName(DE_WorldVoice[2])) else _Notice("Commands", "argument 2 must be a player name!", 1, "SendNotification") end
        else
            _IsSpectatingSomeOne = false
            local _SpectatingBackToSelf = _PLRS.LocalPlayer.Name
            _Spectating(_SpectatingBackToSelf)
        end
    elseif table.find(Commands_DataStorage["followto"], DE_WorldVoice[1]) then
        if DE_WorldVoice[2] then
            _TwPos(_ReturnPlayerPos(_AutoCorrectName(DE_WorldVoice[2]).Name))
            _Notice("Commands", "Currently Following to: " .. _AutoCorrectName(DE_WorldVoice[2]).Name, 1, "SendNotification")
        end
    elseif table.find(Commands_DataStorage["iyedited"], DE_WorldVoice[1]) then
        if not _alreadyRunIYEdited then
            _alreadyRunIYEdited = true
            local success, result = pcall(function()
                loadstring(game:HttpGet("https://raw.githubusercontent.com/HoangHienXScripts/Scripts/refs/heads/modified/InfiniteYield/MainSource.luau"))()
            end)
            if success then
                _Notice("IY-Edited", "Successfully loaded!", 1, "SendNotification")
            else
                _Notice("IY-Edited", "Failed load, error: " .. result, 1, "SendNotification")
            end
        else
            _Notice("Commands", "This command should be only used one time...", 1, "SendNotification")
        end
    elseif table.find(Commands_DataStorage["cmds"], DE_WorldVoice[1]) then
        for i, key in ipairs(_OrderedCommands) do
            local aliases = Commands_DataStorage[key]
            print("Command[" .. i .. "]: " .. key .. " <+> Aliases: " .. table.concat(aliases, ", "))
        end
        _Notice("nah", "nothing here", 1, "DevConsoleVisible")
    elseif table.find(Commands_DataStorage["remotespy"], DE_WorldVoice[1]) then
        _PasteScripts("https://raw.githubusercontent.com/NganHa2k3/RobloxScript/refs/heads/item/Remote_Spy_For_Mobile", function()
            _Notice("Commands", "Successfully loaded!", 1, "SendNotification")
        end)
        --print("Nothing for now!")
        --_Notice("Nah", "Nothing for now!", 1, "DevConsoleVisible")
    elseif table.find(Commands_DataStorage["explorer"], DE_WorldVoice[1]) then
        _PasteScripts("https://raw.githubusercontent.com/NganHa2k3/RobloxScript/refs/heads/item/Dex_Explorer_For_Mobile", function()
            _Notice("Commands", "Successfully loaded!", 1, "SendNotification")
        end)
        --print("Nothing for now!")
        --_Notice("Nah", "Nothing for now!", 1, "DevConsoleVisible")
    elseif table.find(Commands_DataStorage["raid"], DE_WorldVoice[1]) then
        if _FirstSea then _Notice("Commands", "Can't doing RAID at first sea\nso let me just buy it for fun if u had enough money or level...", 1, "SendNotification")
            _RlS.Remotes.CommF_:InvokeServer("RaidsNpc", "Select", DE_WorldVoice[2])
        elseif _SecondsSea then _Notice("Commands", "Starting RAID: " .. DE_WorldVoice[2] .. "...", 1, "SendNotification")
            _RlS.Remotes.CommF_:InvokeServer("RaidsNpc", "Select", DE_WorldVoice[2])
            task.wait(0.05)
            fireclickdetector(_WS.Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)
        elseif _ThirdSea then _Notice("Commands", "Starting RAID: " .. DE_WorldVoice[2] .. "...", 1, "SendNotification")
            _RlS.Remotes.CommF_:InvokeServer("RaidsNpc", "Select", DE_WorldVoice[2])
            task.wait(0.05)
            warn("Current has nothing here...")
        end
    elseif table.find(Commands_DataStorage["setfarmevent"], DE_WorldVoice[1]) then
        if DE_WorldVoice[2] == "lightning" and _SpecialVariables.Current_Event == "LightningEvents" then
            if not _SpecialVariables.Farm_LightningEvent then
                _SpecialVariables.Farm_LightningEvent = true
                _Notice("Commands", "Auto Farm Lightning NPC, Activated/On", 1, "SendNotification")
            else
                _SpecialVariables.Farm_LightningEvent = false
                _Notice("Commands", "Auto Farm Lightning NPC, Deactivated/Off", 1, "SendNotification")
            end
        elseif DE_WorldVoice[2] == nil or DE_WorldVoice[2] == "" and _SpecialVariables.Current_Event == "" then
            _Notice("Commands", "Currently has no event activited!", 1, "SendNotification")
        end
    elseif table.find(Commands_DataStorage["console"], DE_WorldVoice[1]) then
        _Notice("nothing", "nothing", 1, "DevConsoleVisible")
        _Notice("Commands", "Developer Console Opened!", 1, "SendNotification")
    elseif table.find(Commands_DataStorage["maxvision"], DE_WorldVoice[1]) then
        _Notice("Commands", "Observation Haki Vision Radius Has Increase to MAX LEVEL", 1, "SendNotification")
        _PLRS.LocalPlayer.VisionRadius.Value = 90000
        if _PLRS.LocalPlayer:GetAttribute("KenHaki") ~= true then
            firesignal(_PLRS.LocalPlayer.PlayerGui.MobileContextButtons.ContextButtonFrame.BoundActionKen.Button.MouseButton1Down)
        end
    elseif table.find(Commands_DataStorage["savingdump"], DE_WorldVoice[1]) then
        function _checktable(t, time)
            time = time or 0
            local structure = string.rep(" ", time) .. "{\n"
            for a, b in pairs(t) do
                local _istype = type(b)
                if _istype == "table" then
                    structure = structure .. string.rep(" ", time + 4) .. a .. " = {\n"
                    structure = structure .. _checktable(b, time + 8)
                    structure = structure .. string.rep(" ", time + 4) .. "}\n"
                elseif _istype == "function" then
                    structure = structure .. string.rep(" ", time + 4) .. a .. " = function: " .. tostring(b) .. "\n"
                elseif _istype == "boolean" then
                    structure = structure .. string.rep(" ", time + 4) .. a .. " = " .. tostring(b) .. "\n"
                elseif _istype == "number" then
                    structure = structure .. string.rep(" ", time + 4) .. a .. " = " .. b .. "\n"
                elseif _istype == "string" then
                    structure = structure .. string.rep(" ", time + 4) .. a .. " = \"" .. b .. "\"\n"
                elseif _istype == "nil" then
                    structure = structure .. string.rep(" ", time + 4) .. a .. " = nil\n"
                end
            end
            structure = structure .. string.rep(" ", time) .. "}\n"
            return structure
        end
        
        local target = game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("getInventory")
        
        local structure = _checktable(target, 0)
        --writefile(_PLRS.LocalPlayer.Name .. "_DATA_STRUCTURE.txt", structure)
        setclipboard(structure)
        _Notice("Commands", "File saved: " .. _PLRS.LocalPlayer.Name .. "_DATA_STRUCTURE.txt\nCheck your file on device!", 1, "SendNotification")
    elseif table.find(Commands_DataStorage["serverhop"], DE_WorldVoice[1]) then
        _PasteScripts("https://raw.githubusercontent.com/Ancient2k3/Ability/main/ServerHop_Https", function() _Notice("Commands", "SERVER HOPPING IN FEW SECONDS!", 1, "SendNotification") end)
    elseif table.find(Commands_DataStorage["allhaki"], DE_WorldVoice[1]) then
        _GetFightingStyle("CommF_", "Learned Geppo", "BuyHaki", _InvokeText["Haki"][1])
        _GetFightingStyle("CommF_", "Learned Buso", "BuyHaki", _InvokeText["Haki"][2])
        _GetFightingStyle("CommF_", "Learned FlashStep", "BuyHaki", _InvokeText["Haki"][3])
    elseif table.find(Commands_DataStorage["titles"], DE_WorldVoice[1]) then
        if not _PLR.PlayerGui.Main.Titles.Visible then _PLR.PlayerGui.Main.Titles.Visible = true else _PLR.PlayerGui.Main.Titles.Visible = false end
        _Notice("Commands", "Titles UI is now: " .. _PLR.PlayerGui.Main.Titles.Visible, 1, "SendNotification")
    elseif table.find(Commands_DataStorage["awakening"], DE_WorldVoice[1]) then
        local awkui = _PLR.PlayerGui.Main.AwakeningToggler
        if not awkui.Visible then awkui.Visible = true else awkui.Visible = false end
        _Notice("Commands", "Awakening UI is now: " .. awkui.Visible, 1, "SendNotification")
    elseif table.find(Commands_DataStorage["upstat"], DE_WorldVoice[1]) then
        local upstat_statement
        if DE_WorldVoice[2] and DE_WorldVoice[3] then
            if DE_WorldVoice[3] == "true" then
                upstat_statement = true
            else
                upstat_statement = false
            end
            if DE_WorldVoice[2] == "fs" or DE_WorldVoice[2] == "ml" then
                _AutoStatus = upstat_statement or false
                _SpecialVariables.UpStatTarget = "Melee"
            end
        else
            upstat_statement = false
            _AutoStatus = upstat_statement or false
            print("Disabled auto stat!")
            print(DE_WorldVoice[1] .. " [fs, ml/df, sh/sw, bl/pt, de/df, bf] [boolean]")
            _Notice("Commands", "Open Developer Console!", 1, "DevConsoleVisible")
        end
    elseif table.find(Commands_DataStorage["jobid"], DE_WorldVoice[1]) then
        _Clipboard(game.JobId)
        _Notice("Commands", "JobId: " .. game.JobId .. "\n -- Server JobId Saved --", 1, "SendNotification")
    elseif table.find(Commands_DataStorage["autofarmchest"], DE_WorldVoice[1]) then
        _PasteScripts("https://raw.githubusercontent.com/Ancient2k3/Tensura/refs/heads/main/BloxFruits_Update_Dragon_ChestFarm_V001", function() print("-- Running Auto Farming Chest --") end)
    elseif table.find(Commands_DataStorage["loadscript"], DE_WorldVoice[1]) then
        if typeof(DE_WorldVoice[2]) == "string" or type(DE_WorldVoice[2]) == "string" then
            if DE_WorldVoice[2] == "redz" then loadstring(game:HttpGet("https://raw.githubusercontent.com/tlredz/Scripts/refs/heads/main/main.luau"))() else _Notice("Commands", "Not Exist This Script Named " .. DE_WorldVoice[2], 1, "SendNotification") end
        elseif typeof(DE_WorldVoice[2]) == "number" or type(DE_WorldVoice[2]) == "number" then _Notice("Commands", "I'm currently... dumb!", 1, "SendNotification")
        else _Notice("Commands", "Invalid ARGS_2, Type Name!", 1, "SendNotification") end
    elseif table.find(Commands_DataStorage["bringboat"], DE_WorldVoice[1]) then
        --local boat = _ReturnOwnerBoatIs(_StoredBoats)
        --if not boat then
        --    _Notice("Commands", "No boat found to teleport!", 1, "SendNotification")
        --    return
        --end
        print("-- Running 'bringboat' Command! --")
        if DE_WorldVoice[2] == "?" then
            _Notice("Commands", "Usage: bringboat self | bringboat <player> | bringboat x y z | bringboat seadanger5to6 (Third Sea only)", 1, "SendNotification")
        elseif DE_WorldVoice[2] == "self" then
            if _PLR.Character and _PLR.Character:FindFirstChild("HumanoidRootPart") then
                local myPosition = _PLR.Character.HumanoidRootPart.Position
                print(_PLR.DisplayName .. " Position at: " .. tostring(myPosition))
                if _TpBoat_ToPos(myPosition) then
                    _Notice("Commands", "Boat teleported to your position!", 1, "SendNotification")
                else
                    _Notice("Commands", "Failed to teleport boat to your position!", 1, "SendNotification")
                end
            else
                _Notice("Commands", "Your character is not loaded!", 1, "SendNotification")
            end
        elseif DE_WorldVoice[2] == "danger_5" then
            if _ThirdSea then
                local defaultPos = Vector3.new(-42611, 0, 510)
                if _TpBoat_ToPos(defaultPos) then
                    _Notice("Commands", "Boat teleported to SEA Danger 5 nearby 6!", 1, "SendNotification")
                else
                    _Notice("Commands", "Failed to teleport boat to SEA Danger 5 nearby 6!", 1, "SendNotification")
                end
            else
                _Notice("Commands", "The 'danger_5' option is only available in Third Sea!", 1, "SendNotification")
            end
        elseif tonumber(DE_WorldVoice[2]) and tonumber(DE_WorldVoice[3]) and tonumber(DE_WorldVoice[4]) then
            local x, y, z = tonumber(DE_WorldVoice[2]), tonumber(DE_WorldVoice[3]), tonumber(DE_WorldVoice[4])
            local targetPos = Vector3.new(x, y, z)
            if _TpBoat_ToPos(targetPos) then
                _Notice("Commands", "Boat teleported to position: " .. x .. ", " .. y .. ", " .. z, 1, "SendNotification")
            else
                _Notice("Commands", "Failed to teleport boat to position: " .. x .. ", " .. y .. ", " .. z, 1, "SendNotification")
            end
        else
            -- Check if DE_WorldVoice[2] is a player name --
            local targetPlayer = nil
            for _, player in pairs(_PLRS:GetPlayers()) do
                if player.Name:lower():find(DE_WorldVoice[2]:lower()) or player.DisplayName:lower():find(DE_WorldVoice[2]:lower()) then
                    targetPlayer = player
                    break
                end
            end
            if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local targetPos = targetPlayer.Character.HumanoidRootPart.Position
                print(targetPlayer.DisplayName .. " Position at: " .. tostring(targetPos))
                if _TpBoat_ToPos(targetPos) then
                    _Notice("Commands", "Boat teleported to " .. targetPlayer.DisplayName .. "'s position!", 1, "SendNotification")
                else
                    _Notice("Commands", "Failed to teleport boat to " .. targetPlayer.DisplayName .. "'s position!", 1, "SendNotification")
                end
            else
                _Notice("Commands", "Invalid arguments or player not found! Use 'bringboat self', 'bringboat <player>', 'bringboat x y z', or 'bringboat seadanger5to6'", 1, "SendNotification")
            end
        end
    elseif table.find(Commands_DataStorage["refunds"], DE_WorldVoice[1]) then
        if DE_WorldVoice[2] == "code" or DE_WorldVoice[2] == "c" then
            game:GetService("ReplicatedStorage").Remotes.Redeem:InvokeServer("Sub2GamerRobot_Reset1")
            task.wait()
            game:GetService("ReplicatedStorage").Remotes.Redeem:InvokeServer("Sub2UncleKizaru")
            task.wait()
            game:GetService("ReplicatedStorage").Remotes.Redeem:InvokeServer("Kitt_reset")
        elseif DE_WorldVoice[2] == "frags" or DE_WorldVoice[2] == "fragments" or DE_WorldVoice[2] == "f" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BlackbeardReward", "Refund", "2")
            print("Used fragments!")
        end
    elseif table.find(Commands_DataStorage["race"], DE_WorldVoice[1]) then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BlackbeardReward", "Reroll", "2")
        print("Used fragments!")
    elseif table.find(Commands_DataStorage["removehrpbodyvelocity"], DE_WorldVoice[1]) then
        for i, v in pairs(_PLR.Character.HumanoidRootPart:GetChildren()) do
            if v:IsA("BodyVelocity") and v.Name == "Extra_Wings_IX" then
                v:Destroy()
                warn("Totaly destroyed " .. i .. " body velocity.")
            end
        end
    elseif table.find(Commands_DataStorage["melee"], DE_WorldVoice[1]) then
        if DE_WorldVoice[2] == "darkstep" or DE_WorldVoice[2] == "dsv1" then
            _GetFightingStyle("CommF_", "Dark Step", _InvokeText["FightingStyle"][1])
        elseif DE_WorldVoice[2] == "electro" or DE_WorldVoice[2] == "elv1" then
            _GetFightingStyle("CommF_", "Electro", _InvokeText["FightingStyle"][2])
        elseif DE_WorldVoice[2] == "fishman" or DE_WorldVoice[2] == "fm" then
            _GetFightingStyle("CommF_", "Fishman Karate", _InvokeText["FightingStyle"][3])
        elseif DE_WorldVoice[2] == "dragonbreath" or DE_WorldVoice[2] == "db" then
            _GetFightingStyle("CommF_", "Dragon Breath", "BlackbeardReward", _InvokeText["FightingStyle"][4], "2")
        elseif DE_WorldVoice[2] == "superhuman" or DE_WorldVoice[2] == "sphm" then
            _GetFightingStyle("CommF_", "Super Human", _InvokeText["FightingStyle"][5])
        elseif DE_WorldVoice[2] == "deathstep" or DE_WorldVoice[2] == "dsv2" then
            _GetFightingStyle("CommF_", "Death Step", _InvokeText["FightingStyle"][6])
        elseif DE_WorldVoice[2] == "electric" or DE_WorldVoice[2] == "elv2" then
            _GetFightingStyle("CommF_", "Electric Claw", _InvokeText["FightingStyle"][7])
        elseif DE_WorldVoice[2] == "sharkman" or DE_WorldVoice[2] == "sm" then
            _GetFightingStyle("CommF_", "Sharkman Karate", _InvokeText["FightingStyle"][8])
        elseif DE_WorldVoice[2] == "dragontalon" or DE_WorldVoice[2] == "dt" then
            _GetFightingStyle("CommF_", "Dragon Talon", _InvokeText["FightingStyle"][9])
        elseif DE_WorldVoice[2] == "godhuman" or DE_WorldVoice[2] == "gh" then
            _GetFightingStyle("CommF_", "God Human", _InvokeText["FightingStyle"][10])
        elseif DE_WorldVoice[2] == "saguine" or DE_WorldVoice[2] == "sg" then
            _GetFightingStyle("CommF_", "Saguine Art", _InvokeText["FightingStyle"][11])
        elseif DE_WorldVoice[2] == "?" then
            print("All FightingStyle Code:\n • Normal: darkstep, electro, fishman, dragonbreath, superhuman, deathstep, electric, sharkman, dragontalon, godhuman, saguine \n • Short: dsv1, elv1, fm, db, sphm, dsv2, elv2, sm, dt, gh, sg")
            _Notice("Nah", "Nothing Here!", 1, "DevConsoleVisible")
        end
    elseif table.find(Commands_DataStorage["pvp"], DE_WorldVoice[1]) then
        if _AutoEnablePvp then
            _AutoEnablePvp = false
            _Notice("Commands", "Auto Enable Pvp is Deactivated!", 1, "SendNotification")
        else
            _AutoEnablePvp = true
            _Notice("Commands", "Auto Enable Pvp is Activated!", 1, "SendNotification")
