    if isIfQC then
        QCombo.TextColor3 = Color3.fromRGB(255, 255, 255)
        SandSandQuickly.Visible = false
        isIfQC = false
    else
        QCombo.TextColor3 = Color3.fromRGB(0, 255, 0)
        SandSandQuickly.Visible = true
        isIfQC = true
    end
end)
