local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")

local function findNearestHumanoidRootPart(position)
    local nearestHumanoidRootPart = nil
    local nearestDistance = math.huge
    local playerCharacter = player.Character

    local function searchFolder(folder)
        for _, object in ipairs(folder:GetChildren()) do
            if object:IsA("Model") and object:FindFirstChild("HumanoidRootPart") and object ~= playerCharacter then
                local humanoidRootPart = object.HumanoidRootPart
                local distance = (humanoidRootPart.Position - position).magnitude

                if distance < nearestDistance then
                    nearestDistance = distance
                    nearestHumanoidRootPart = humanoidRootPart
                end
            end

            if object:IsA("Folder") then
                searchFolder(object)
            end
        end
    end

    searchFolder(Workspace)

    return nearestHumanoidRootPart
end

local function abilityfire(targetHumanoidRootPart)
    local args = {
        [1] = "DF_SandSand_Z",
        [2] = {
            ["Type"] = "Down",
            ["MouseHit"] = CFrame.new(targetHumanoidRootPart.Position) * CFrame.Angles(0, 0, 0)
        }
    }

    ReplicatedStorage.Chest.Remotes.Functions.SkillAction:InvokeServer(unpack(args))
    wait(0.01)

    local args = {
        [1] = "DF_SandSand_Z",
        [2] = {
            ["Type"] = "Up",
            ["MouseHit"] = CFrame.new(targetHumanoidRootPart.Position) * CFrame.Angles(0, 0, 0)
        }
    }

    ReplicatedStorage.Chest.Remotes.Functions.SkillAction:InvokeServer(unpack(args))
    wait(0.01)

    local args = {
        [1] = "DF_SandSand_V",
        [2] = {
            ["Type"] = "Down",
            ["MouseHit"] = CFrame.new(targetHumanoidRootPart.Position) * CFrame.Angles(0, 0, 0)
        }
    }

    ReplicatedStorage.Chest.Remotes.Functions.SkillAction:InvokeServer(unpack(args))
    wait(0.01)

    local args = {
        [1] = "DF_SandSand_V",
        [2] = {
            ["Type"] = "Up",
            ["MouseHit"] = CFrame.new(targetHumanoidRootPart.Position) * CFrame.Angles(0, 0, 0)
        }
    }

    ReplicatedStorage.Chest.Remotes.Functions.SkillAction:InvokeServer(unpack(args))
    wait(0.01)

    local args = {
        [1] = "DF_SandSand_X",
        [2] = {
            ["Type"] = "Down",
            ["MouseHit"] = CFrame.new(targetHumanoidRootPart.Position) * CFrame.Angles(0, 0, 0)
        }
    }

    ReplicatedStorage.Chest.Remotes.Functions.SkillAction:InvokeServer(unpack(args))
    wait(0.01)

    local args = {
        [1] = "DF_SandSand_X",
        [2] = {
            ["Type"] = "Up",
            ["MouseHit"] = CFrame.new(targetHumanoidRootPart.Position) * CFrame.Angles(0, 0, 0)
        }
    }

    ReplicatedStorage.Chest.Remotes.Functions.SkillAction:InvokeServer(unpack(args))
    wait(0.01)
end

local SandSandQuickly = Instance.new("TextButton")
SandSandQuickly.Parent = KingLegacySG
SandSandQuickly.Size = UDim2.new(0.05, 0, 0.1, 0)
SandSandQuickly.Position = UDim2.new(0.15, 0, 0.3, 0)
SandSandQuickly.Text = "Use[Sand Ability]"
SandSandQuickly.Font = Enum.Font.Code
SandSandQuickly.TextScaled = true
SandSandQuickly.TextColor3 = Color3.new(1, 0, 1)
SandSandQuickly.BackgroundColor3 = Color3.new(0, 0, 0)
SandSandQuickly.BackgroundTransparency = 0.5
SandSandQuickly.Visible = false

SandSandQuickly.MouseButton1Click:Connect(function()
    local playerHumanoidRootPart = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
    local playerPosition = playerHumanoidRootPart and playerHumanoidRootPart.Position

    if playerPosition then
        local nearestHumanoidRootPart = findNearestHumanoidRootPart(playerPosition)

        if nearestHumanoidRootPart then
            abilityfire(nearestHumanoidRootPart)
        else
            abilityfire(playerHumanoidRootPart)
        end
    else
        print("Player's HumanoidRootPart not found.")
    end
end)

