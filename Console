local ConsoleLog_Button_SG = Instance.new("ScreenGui")
ConsoleLog_Button_SG.Name = "QuickOpen_Console"
ConsoleLog_Button_SG.Parent = game.CoreGui

local success, message = pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NganHa2k3/Tensura/ciel/Keypressing_X"))()
end)
if not success then
    warn("Error loading external script: " .. message)
end

_G.Pressing = _G.Pressing or function(key)
    print("Key pressed: " .. key)
end

local f9_Button = Instance.new("TextButton")
f9_Button.Parent = ConsoleLog_Button_SG
f9_Button.BackgroundColor3 = Color3.new(0, 0, 0)
f9_Button.BackgroundTransparency = 0.5
f9_Button.Size = UDim2.new(0.1, 0, 0.1, 0)
f9_Button.TextScaled = true
f9_Button.Position = UDim2.new(0.35, 0, -0.1, 0)
f9_Button.TextColor3 = Color3.new(1, 1, 1)
f9_Button.Font = Enum.Font.Code
f9_Button.Text = "F9"

f9_Button.MouseButton1Click:Connect(function()
    _G.Pressing("f9")
    print("<Pressing F9>")
    
    if game:GetService("CoreGui"):FindFirstChild("DeveloperConsole") == nil then
        game:GetService("StarterGui"):SetCore("DevConsoleVisible", true)
    end
end)

f9_Button.MouseEnter:Connect(function()
    f9_Button.TextColor3 = Color3.new(1, 0, 0)
end)

f9_Button.MouseLeave:Connect(function()
    f9_Button.TextColor3 = Color3.new(1, 1, 1)
end)
